<h3>Student Form</h3>

  <%= form_tag "/students", method: :post do %>
    <label>First Name:</label><br>
    <input type="text" name="student[first_name]" id="first_name"><br>

    <label>Last Name:</label><br>
    <input type="text" name="student[last_name]" id="last_name"><br>

    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <input type="submit" value="Submit Student">
  <% end %>


